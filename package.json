{
  "name": "notes-dashboard",
  "version": "0.0.1",
  "description": "A simple markdown post-it dashboard",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js && NODE_ENV=production npm run build:db",
    "build:db": "export NODE_ENV=production && npm run db:migrate && npm run db:seed",
    "dev": "nodemon src/server/index.js",
    "dev:db": "knex migrate:latest && knex seed:run",
    "test": "jest",
    "db:seed": "knex seed:run",
    "db:migrate": "knex migrate:latest"
  },
  "author": "Cristian Chelu",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^4.5.2",
    "knex": "^0.95.4",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-webpack": "^6.0.0",
    "markdown-to-jsx": "^7.1.2",
    "mini-css-extract-plugin": "^1.4.0",
    "objection": "^2.2.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "sass-loader": "^10.1.1",
    "sqlite3": "^5.0.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/eslint-plugin": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.32.8",
    "supertest": "^6.1.3",
    "webpack-hot-client": "^4.2.0"
  }
}
